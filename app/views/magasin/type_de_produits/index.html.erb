<h1>Magasin::TypeDeProduits#index</h1>
<p>Find me in app/views/magasin/type_de_produits/index.html.erb</p>


<a><%= link_to "New Type of produit", new_magasin_magasin_type_de_produit_path(@vendeur) %></a>
<h1>Types of produit in magasin <%= @vendeur.magasin.try(:nom_magasin) %></h1>
<table class="table table-bordered">
  <thead>
    <tr>
      <th >Nom du type</th>
    </tr>
  </thead>
  <tbody>
    <% @types.each do |t| %>
      <tr>
        <td><%= t.type_produit %></td>
      </tr>
    <% end %>
  </tbody>
</table>